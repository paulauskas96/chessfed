{
	"$schema": "https://schemas.wp.org/wp/6.3/theme.json",
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "Poppins Black",
					"slug": "poppins-black",
					"fontFace": [
						{
							"fontFamily": "Poppins Black",
							"fontWeight": "900",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/poppins-black_normal_900.ttf"
							]
						},
						{
							"fontFamily": "Poppins Black",
							"fontWeight": "900",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/poppins-black_italic_900.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Poppins ExtraBold",
					"slug": "poppins-extrabold",
					"fontFace": [
						{
							"fontFamily": "Poppins ExtraBold",
							"fontWeight": "800",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/poppins-extrabold_normal_800.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Poppins ExtraLight",
					"slug": "poppins-extralight",
					"fontFace": [
						{
							"fontFamily": "Poppins ExtraLight",
							"fontWeight": "275",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/poppins-extralight_normal_275.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Poppins Light",
					"slug": "poppins-light",
					"fontFace": [
						{
							"fontFamily": "Poppins Light",
							"fontWeight": "300",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/poppins-light_normal_300.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Poppins Medium",
					"slug": "poppins-medium",
					"fontFace": [
						{
							"fontFamily": "Poppins Medium",
							"fontWeight": "500",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/poppins-medium_normal_500.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Poppins",
					"slug": "poppins",
					"fontFace": [
						{
							"fontFamily": "Poppins",
							"fontWeight": "700",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/poppins_normal_700.ttf"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontWeight": "400",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/poppins_normal_400.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Poppins SemiBold",
					"slug": "poppins-semibold",
					"fontFace": [
						{
							"fontFamily": "Poppins SemiBold",
							"fontWeight": "600",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/poppins-semibold_normal_600.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Poppins Thin",
					"slug": "poppins-thin",
					"fontFace": [
						{
							"fontFamily": "Poppins Thin",
							"fontWeight": "250",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/poppins-thin_normal_250.ttf"
							]
						}
					]
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2
}